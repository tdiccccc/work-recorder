<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-10">
      <h4 class="bg-light px-2 my-5">工程名編集</h4>
      <%= form_with model: [:admin,@process] do |f| %>
        <div class="row my-4 form-group mb-5">
          <%= f.label "工程名", class: "col-sm-2 col col-form-label ml-3" %>
          <div class="col-sm-6 col">
          <%= f.text_field :name, class: "form-control" %>
          </div>
          <%= f.submit "変更を保存", class: "btn btn-success mr-3" %>
        </div>
      <%end%>
    </div>
  </div>
</div>